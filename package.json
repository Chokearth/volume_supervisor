    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
